{
  "compilerOptions": {
    "module": "NodeNext",
    "esModuleInterop": true,
    "moduleResolution": "nodenext",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "baseUrl": "./functions/src/routes",
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "es2017"
  },
  "compileOnSave": true,
  "include": ["."]
}
