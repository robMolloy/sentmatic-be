{
  "name": "functions",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "functions:build": "tsc",
    "functions:build:watch": "tsc --watch",
    "firebase:emulator": "node_modules/.bin/firebase emulators:start --project demo-project --only auth,firestore,functions,storage",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test:watch": "jest --watchAll --detectOpenHandles",
    "dev1": "npm run functions:build:watch",
    "dev2": "npm run firebase:emulator",
    "dev3": "npm run test:watch"
  },
  "engines": {
    "node": "22"
  },
  "main": "lib/index.js",
  "dependencies": {
    "dotenv": "^16.4.7",
    "firebase": "^11.3.1",
    "firebase-admin": "^12.6.0",
    "firebase-functions": "^6.3.2",
    "stripe": "^17.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^4.0.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "firebase-tools": "^13.30.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "typescript": "^4.9.0"
  },
  "private": true
}
